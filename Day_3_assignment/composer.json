{
  "name": "app/training-sessions",
  "description": "Training Sessions Management (CakePHP 3.8)",
  "require": {
    "php": ">=7.1",
    "cakephp/cakephp": "~3.8.0",
    "cakephp/migrations": "^2.0",
    "cakephp/bake": "^1.0"
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    },
    "audit": {
      "block-insecure": false
    },
    "allow-plugins": {
      "cakephp/plugin-installer": true
    }
  },  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "cakephp/debug_kit": "^3.23",
    "phpunit/phpunit": "9.5"
  },
  "scripts": {
    "test": "phpunit",
    "migrate": "bin/cake migrations migrate",
    "serve": "php -S 127.0.0.1:8765 -t webroot"
  }
}
